{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "src": "SharePointServer",
        "dest": ".",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "src": "SharePointOnline",
        "dest": ".",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "src": "Hub",
        "dest": ".",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.chm",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "markdownEngineName": "markdig",    
    "globalMetadata": {
      "breadcrumb_path": "/sharepoint/breadcrumb/toc.json",
      "extendBreadcrumb": "true",
      "contributors_to_exclude": [
        "dstrome",
        "daswifty",
        "Reraghavan",
        "denisebmsft"
      ],
      "searchScope": ["SharePoint Server"],
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/OfficeDocs-SharePoint",
      "feedback_product_url": "https://sharepoint.uservoice.com/" 
    },
    "fileMetadata": {
        "feedback_product_url": { 
            "SharePointServer/search/*.md": "https://sharepoint.uservoice.com/forums/330321-sharepoint-search" },
        "searchScope": { 
            "SharePointOnline/*.md": ["SharePoint Online"] }},
    "template": [],
    "dest": "OfficeDocs-SharePoint"
  }
}
